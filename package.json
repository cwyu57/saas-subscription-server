{
  "name": "saas-subscription-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "dev": "nodemon --config nodemon.json --exec NODE_ENV=development ts-node -r dotenv-flow/config src/server.ts",
    "staging": "nodemon --config nodemon.json --exec NODE_ENV=staging ts-node -r dotenv-flow/config src/server.ts",
    "docker-build-dev": "docker build -t saas-subscription-server .",
    "docker-run-dev": "docker run --rm --env NODE_ENV=development --env DB_HOST=host.docker.internal --env-file=.env.development.local -v $PWD/local:/usr/src/app/local -p 8080:8080 saas-subscription-server",
    "format": "prettier --write .",
    "lint": "eslint --fix 'src/**/*.ts'",
    "start": "node -r dotenv-flow/config dist/server.js",
    "test": "NODE_ENV=jest jest --testTimeout=30000 --detectOpenHandles",
    "seed:all": "npm run build && npx --no-install dotenv-flow sequelize db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "dotenv-flow-cli": "^1.0.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-basic-auth": "^1.2.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0",
    "yamljs": "^0.3.0"
  }
}
